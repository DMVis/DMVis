{
  "name": "@dmvis/dmvis",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest --coverage",
    "coverage:ui": "vitest --coverage --ui",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "prepare": "husky",
    "lint-staged": "lint-staged",
    "docs": "docsify serve documentation"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DMVis/DMVis.git"
  },
  "author": "DMVis",
  "license": "AGPL-3.0-only",
  "homepage": "https://github.com/DMVis/DMVis#readme",
  "exports": {
    ".": {
      "types": "./dist/Index.d.ts",
      "svelte": "./dist/Index.js"
    },
    "./components": {
      "types": "./dist/Components.d.ts",
      "svelte": "./dist/Components.js"
    },
    "./utils": {
      "types": "./dist/Utils.d.ts",
      "svelte": "./dist/Utils.js"
    },
    "./enums": {
      "types": "./dist/Enums.d.ts",
      "svelte": "./dist/Enums.js"
    },
    "./types": {
      "types": "./dist/Types.d.ts",
      "svelte": "./dist/Types.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "jest": {
    "testEnvironment": "jsdom"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --quiet --fix"
    ],
    "*.{json,js,ts,html,svelte,md}": [
      "prettier --write --ignore-unknown"
    ]
  },
  "peerDependencies": {
    "svelte": "^4.0.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/svelte": "^4.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/chroma-js": "^2.4.4",
    "@types/colorbrewer": "^1.0.32",
    "@types/d3": "^7.4.3",
    "@types/eslint": "8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.4.0",
    "@vitest/ui": "^1.4.0",
    "docsify-cli": "^4.4.4",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.1.2",
    "publint": "^0.1.9",
    "svelte-check": "^3.6.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.11",
    "vitest": "^1.2.0"
  },
  "svelte": "./dist/Index.js",
  "types": "./dist/Index.d.ts",
  "type": "module",
  "dependencies": {
    "chroma-js": "^2.4.2",
    "colorbrewer": "^1.5.6",
    "d3": "^7.8.5",
    "svelte": "^4.2.7"
  }
}
